        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "allowSyntheticDefaultImports": true,
        "lib": [
            "ES2017"
        ],
        "types": [
            "node", "express"
        ]
    },
    "include": [
        "src/server/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}